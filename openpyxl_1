from openpyxl import load_workbook
data = []
#copy
Workbook = load_workbook(filename="latihan.xlsx")
ws = Workbook.active
jml = len(ws["A"])
for i in range(1,jml+1):
    d = ws[f'A{i}']      #mengeluarkan nilai/data yg ada di tiap2 cell dari file latihan.xlsx
    data.append(d)       #memindahkan nila/data dari cell ke list
a = 1
for c in data:
    ws[f"D{a}"] = c.value #mengcopy value/nilai dari cell ke colom D
    a +=1
Workbook.save(filename="hasil.xlsx") #menyimpan data ke file baru
